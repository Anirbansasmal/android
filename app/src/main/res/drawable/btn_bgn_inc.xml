<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
    <corners
        android:radius="40dp"
        android:bottomRightRadius="40dp"
        android:topRightRadius="40dp"
        />
    <gradient
        android:angle="45"
        android:centerX="35%"
        android:centerColor="#33a9f5"
        android:startColor="#33a9f5"
        android:endColor="#33a9f5"
        android:type="linear"
        />
    <padding
        android:left="0dp"
        android:top="0dp"
        android:right="0dp"
        android:bottom="0dp"
        />
    <size
        android:width="100px"
        android:height="80px"
        />
    <stroke
        android:width="0dp"
        android:color="#AECE3E3E"
        />
</shape>