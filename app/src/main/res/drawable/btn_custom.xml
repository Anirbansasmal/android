<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <corners
        android:radius="50px"
        android:bottomRightRadius="50px"
        android:topRightRadius="51px"
        />
    <gradient
        android:angle="45"
        android:centerX="35%"
        android:centerColor="#03A9F4"
        android:startColor="#FCFAF8"
        android:endColor="#FDFAF7"
        android:type="linear"
        />
    <padding
        android:left="10dp"
        android:top="10dp"
        android:right="10dp"
        android:bottom="10dp"
        />
    <size
        android:width="80px"
        android:height="80px"
        />
    <stroke
        android:width="1px"
        android:color="#2196F3"
        />
</shape>